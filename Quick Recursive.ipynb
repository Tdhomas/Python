    "def Recursive_Factoriel(n):\n",
    "    if(n==0 or n==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n*Recursive_Factoriel(n-1)\n",
    "print (Recursive_Factoriel(6))\n",
    "print(2*3*4*5*6)\n",
    "print (Recursive_Factoriel(0))"
   ]
